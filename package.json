{
    "name": "emoji-uuid-api",
    "version": "1.0.0",
    "description": "A simple converter between serial codes/alphanumeric code and emojis",
    "main": "dist/server.js",
    "scripts": {
        "start": "node .",
        "prebuild": "npm run lint && npm run test",
        "build": "rimraf ./dist && tsc",
        "dev": "ts-node-dev src/server.ts",
        "test": "jest --config jestconfig.json",
        "lint": "eslint . --ext .ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sudo-vaibhav/emoji-uuid.git"
    },
    "author": "Vaibhav Chopra",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sudo-vaibhav/emoji-uuid/issues"
    },
    "homepage": "https://github.com/sudo-vaibhav/emoji-uuid#readme",
    "dependencies": {
        "dotenv": "^8.2.0",
        "emoji-uuid": "^1.0.0",
        "express": "^4.17.1",
        "morgan": "^1.10.0",
        "rimraf": "^3.0.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.8",
        "@types/morgan": "^1.9.1",
        "@types/node": "^14.11.8",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.4.0",
        "eslint": "^7.11.0",
        "eslint-plugin-no-loops": "^0.3.0",
        "jest": "^26.5.3",
        "supertest": "^5.0.0",
        "ts-jest": "^26.4.1",
        "ts-node-dev": "^1.0.0-pre.63",
        "typescript": "^4.0.3"
    }
}
